{
  "name": "atomic-layout",
  "version": "0.4.5",
  "description": "A single component to distribute a spacial relation in your layouts using CSS Grid.",
  "license": "MIT",
  "main": "./index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack --watch",
    "storybook": "start-storybook -p 6020",
    "build": "webpack",
    "cypress": "node_modules/.bin/cypress open",
    "examples": "webpack-dev-server --config examples/webpack.config.js",
    "flow": "node_modules/.bin/flow",
    "test:unit": "cross-env BABEL_ENV=test jest",
    "cypress:cli": "node_modules/.bin/cypress run --spec cypress/integration/index.js --browser chrome",
    "test:e2e": "server-test examples http-get://localhost:9987 cypress:cli",
    "test": "yarn run test:unit && yarn run test:e2e",
    "precommit": "lint-staged",
    "prepush": "yarn run test:unit",
    "preversion": "yarn run build && yarn test"
  },
  "lint-staged": {
    ".{js,jsx}": [
      "eslint ./src/",
      "prettier --write",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./index.js",
      "maxSize": "25 kB"
    }
  ],
  "author": {
    "name": "Artem Zakharchenko",
    "email": "kettanaito@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kettanaito/atomic-layout"
  },
  "keywords": [
    "atomic",
    "layout",
    "responsive",
    "composition",
    "css grid",
    "react"
  ],
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "styled-components": "^3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@cypress/webpack-preprocessor": "^1.1.3",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "bundlesize": "^0.17.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cypress": "^3.0.3",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.89.0",
    "husky": "^1.0.0-rc.9",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "start-server-and-test": "^1.7.11",
    "storybook": "^1.0.0",
    "styled-components": "^4.1.3",
    "url": "^0.11.0",
    "webpack": "^4.26.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "react-responsive": "^5.0.0"
  }
}
