{
  "compileOnSave": true,
  "compilerOptions": {
    "types": ["jest"],
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    // "strict": true,
    "noImplicitThis": false,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
